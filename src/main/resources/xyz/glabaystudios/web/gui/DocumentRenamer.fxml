<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="150.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="xyz.glabaystudios.web.gui.DocumentRenamer">
   <HBox layoutX="2.0" layoutY="63.0" prefHeight="31.0" prefWidth="287.0">
      <opaqueInsets>
         <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
      </opaqueInsets>
      <Button fx:id="imgBtn" mnemonicParsing="false" onAction="#openImageRenamer" prefHeight="31.0" prefWidth="96.0"
              text="Images"/>
      <Button fx:id="wordBtn" mnemonicParsing="false" onAction="#openWordDocRenamer" prefHeight="31.0"
              prefWidth="96.0" text="Word Docx"/>
      <Button fx:id="pdfBtn" mnemonicParsing="false" onAction="#openPdfRenamer" prefHeight="31.0" prefWidth="96.0"
              text="PDF Files"/>
   </HBox>
   <Button fx:id="cxlBtn" layoutX="119.0" layoutY="121.0" mnemonicParsing="false" onAction="#appendWindowClose"
           text="Discard"/>
   <Label alignment="CENTER" layoutX="1.0" layoutY="22.0" prefHeight="17.0" prefWidth="290.0"
          text="Choose a file type you're looking to bulk rename" textAlignment="CENTER">
      <font>
         <Font name="System Italic" size="13.0"/>
      </font>
   </Label>
</AnchorPane>
